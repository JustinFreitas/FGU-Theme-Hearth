<root>
<!-- fgpro override CoreRPG & 5E ruleset graphics -->
	
	<!-- Effects -->
	<template name="label_effects_conditions">
		<label name="label_conditions">
			<anchored to="contentanchor" width="170" height="20">
				<top anchor="bottom"/>
				<left />
			</anchored>
			<static textres="effect_label_conditions" />
			<color>40431E</color>
			<center />
		</label>
	</template>

	<template name="label_effects_custom">
		<label name="label_custom">
			<anchored to="contentanchor" height="20">
				<top anchor="bottom" />
				<left offset="195" />
				<right offset="-10" />
			</anchored>
			<static textres="effect_label_custom" />
			<color>40431E</color>
			<center />
		</label>
	</template>

	<!-- Assets -->
	<windowclass name="tokenbag" version="&gt;=4" merge="join">
		<placement>
			<size width="495" height="585" />
		</placement>
		<sizelimits>
			<dynamic />
			<minimum width="390" height="455" />
		</sizelimits>
	</windowclass>
</root>
