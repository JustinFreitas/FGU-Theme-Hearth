	<!--Ext: Combat Stats-->		  		  
		<!--Frames-->
			<template name="number_ps_stat_lower">
           		<basicnumber>
           			<frame></frame>
            		<anchored to="portraitbase" width="50" height="20">
                    	<top offset="20" />
                    	<left parent="portraitbase" anchor="right" relation="relative" offset="5" />
            		</anchored>
            		<readonly />
            		<hideonvalue > 0 </hideonvalue>
            		<color>69B7FF</color> <!-- Replaceable Color: Temporary HP -->
            		<nodrag />
            		<nolinkwidget />
            			<script>
                    	function onValueChanged()
                        	node=getDatabaseNode();
                       		stat=getName();
                        	value=getValue();
                        	MNMStats.editStatFromPS(node,stat,value);
                    	end
            			</script>
            	</basicnumber>
    		</template>

    		<template name="string_ps_stat_lower">
            		<stringfield>
            		<anchored to="portraitbase" width="150" height="20">
                    		<top offset="20" />
                    		<left parent="portraitbase" anchor="right" relation="relative" offset="5" />
            		</anchored>
            		<color>69B7FF</color> <!-- Replaceable Color: Temporary HP -->
            		<readonly />
            		<nodrag />
            		<script>
                   		function onValueChanged()
                       		node=getDatabaseNode();
                        		stat=getName();
                        		value=getValue();
                        		MNMStats.editStatFromPS(node,stat,value);
                    		end
            		</script>
            		</stringfield>
    		</template>
